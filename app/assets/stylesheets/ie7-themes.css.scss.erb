* {
  z-index: 0!important; }

img {
  -ms-interpolation-mode: bicubic; }

#thumbNav {
  padding-bottom: 5px; }

#filter-bar .dropdown ul {
  margin-left: 0px;
  li a {
    cursor: pointer; } }

#navigation {
  height: 50px; }

/*megadropdown hack to get the z-indexs working properly*/

#filter-bar {
  position: relative;
  z-index: 2; }

#content, #feature {
  position: relative;
  z-index: 1; }

#filter-bar ul.drop li a {
  height: 32px; }

#thumbNav a {
  padding-bottom: 5px; }

.reviews {
  margin-top: 60px; }

#filter-bar .dropdown em {
  float: left;
  top: 3px; }

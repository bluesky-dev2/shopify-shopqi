/* If you make a mess of this stylesheet Mark will super glue your macbook lid and Daniel will watch */

/* SWATCHES
#212121 - [dark grey] body text
#3084ca - [blue] link colour
#78b9e7 - [dark blue] hover link
#d9d9d9 - [light grey] borders
#777 - [light grey] for text
*/

* {
  margin: 0; }

html, body {
  height: 100%; }

/* =============== */
/* = Global CSS = */
/* =============== */

* {
  margin: 0;
  padding: 0; }

html {
  height: 100%; }

body {
  height: 100%;
  background: #eef2e4;
  font-family: 'Lucida Grande', 'Lucida Sans Unicode',sans-serif;
  font-size: 13px;
  color: #212121;
  margin: 0;
  -webkit-text-size-adjust: none; }

/* sticky footer */

h1, h2, h3, h4, h5, h6 {
  font-family: 'Lucida Grande', 'Lucida Sans Unicode';
  font-weight: normal;
  color: #212121; }

h1 {
  text-shadow: 1px 1px 1px #5f8717;
  font-weight: 800;
  font-size: 52px;
  color: #FFF;
  letter-spacing: -1px;
  margin-top: 2px;
  float: left;
  line-height: normal;
  position: relative;
  z-index: 10001; }

h2 {
  font-size: 30px;
  margin: 0 0 25px;
  &.small {
    font-size: 22px;
    margin-bottom: 15px; } }

h3 {
  font-size: 16px;
  margin: 0 0 10px;
  font-weight: bold; }

h4 {
  font-size: 14px;
  margin: 0 0 10px;
  font-weight: bold; }

input {
  margin: 0 0 10px 0; }

p {
  line-height: 20px;
  margin: 0 0 20px 0; }

p+p {
  margin-bottom: 20px; }

ul {
  margin: 0px 0 25px 0px;
  padding: 0 0 0 10px;
  li {
    margin: 0 15px 10px 0;
    background: url(<%= asset_path 'icons/greybullet.gif' %>) no-repeat top left;
    padding: 0 0 0 15px;
    list-style-type: none; } }

a {
  color: #3084ca;
  text-decoration: none;
  font-weight: normal;
  &:hover {
    color: #78b9e7; } }

img {
  border: none; }

form {
  margin: 15px 0 0 0;
  padding: 5px 25px 15px 15px;
  background: #efefef;
  border: 1px solid #d9d9d9;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  label {
    display: block;
    font-size: 12px;
    margin: 15px 0 5px 0;
    color: #000;
    font-weight: bold; }
  textarea {
    width: 98%;
    font-family: helvetica, arial, sans-serif;
    padding: 7px;
    font-size: 14px; }
  select {
    font-size: 13px;
    padding: 1px;
    margin: 4px 0 10px 0; }
  input {
    &[type="text"], &[type="password"] {
      width: 100%;
      padding: 3px;
      font-size: 14px; }
    &[type="image"] {
      margin: 10px 0 0 0;
      display: block; } } }

/*used for contact.html*/

/*custom classes */

.screenshot {
  margin-bottom: 18px;
  img {
    border: 1px solid #ccc;
    padding: 1px; } }

.icon {
  margin: 0 10px 10px 10px; }

/* DEPRECATED - use icon-left or icon-right */

.icon-left {
  float: left;
  margin: 0 10px 10px 0; }

.icon-right {
  float: right;
  margin: 0 0 10px 10px; }

.quote {

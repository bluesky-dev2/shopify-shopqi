    color: #000; } }

/* segmented buttons */

.segmented {
  list-style-type: none;
  position: relative;
  bottom: 56px;
  right: 4px;
  float: right;
  margin-left: 20px;
  li {
    float: left;
    padding: 0;
    margin: 0;
    .btn {
      float: left;
      font-size: 11px;
      border-radius: 0;
      border-left: 0px; }
    &:first-child .btn {
      border-radius: 5px 0 0 5px; }
    &:last-child .btn {
      border-radius: 0px 5px 5px 0; }
    .btn {
      &.active {
        background: #e0e0e0;
        background: -moz-linear-gradient(top, #e0e0e0 0%, #ebebeb 100%);
        background: -webkit-linear-gradient(top, #dcdcdc 0%, #e8e8e8 100%); }
      &:after {
        content: '';
        position: absolute;
        top: 2px;
        border-right: 1px solid #f7f7f7;
        height: 24px;
        right: 0px;
        z-index: 1; }
      &.active:after {
        right: -2px; } }
    &:last-child .btn:after, .btn:hover:after {
      border-right: none; } } }


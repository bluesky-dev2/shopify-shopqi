  &.active>ul li, &.active span {
    opacity: 1; }
  &.active>ul li:hover {} }

#table-actions>ul li {
  &:first-child {
    border-radius: 3px 0 0 3px; }
  &:last-child {
    border-radius: 0 3px 3px 0; }
  &:after {
    content: "";
    position: absolute;
    bottom: -1px;
    width: 90%;
    display: block;
    border-bottom: 1px solid #f7f7f7;
    left: 0px; }
  &:first-child:before {
    border: 0; }
  &:before {
    content: "";
    position: absolute;
    bottom: 0px;
    height: 100%;
    display: block;
    border-right: 1px solid #f9f9f9;
    left: 0px; } }

#table-actions {

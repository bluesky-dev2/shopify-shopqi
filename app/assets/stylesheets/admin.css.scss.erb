/* clearfix */

.clearfix {
  &:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    height: 0;
    line-height: 0; }
  display: inline-block; }

* html .clearfix {
  height: 1%; }

html[xmlns] .clearfix {
  display: block; }

form, input {
  padding: 0;
  margin: 0; }

address {

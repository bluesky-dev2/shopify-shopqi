.clearfix {
  zoom: 1;
  /* triggers hasLayout */
  display: block;
  /* resets display for IE/Win */ }

/*The frontpage mark made with the sugar video*/

.frontpage-navigation ul {
  margin-top: 3px; }

.frontpage-video h1 {
  margin-bottom: 30px;
  margin-top: 40px; }

.roundedcorners-dark, .roundedcorners-green {
